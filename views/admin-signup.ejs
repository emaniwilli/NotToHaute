        <!--Webpage content-->
        <section>
        <div class="signup">
            <h4 class="main-heading">
                Request account registration:
            </h4>
            <form class="form-container" method="post" action="/admin/request-signup">
                <label>First Name:</label><br>
                <input type="text" autocomplete="off" name="FirstName" id="FirstName"><br>
                <label>Last Name:</label><br>
                <input type="text" autocomplete="off" name="LastName" id="LastName"><br>
                <label>Email Address:</label><br>
                <input type="email" autocomplete="email" name="Email" id="Email"><br>
                <label>Password:</label><br>
                <input type="password" autocomplete="off" name="Password" id="Password"><br>
                <button>
                    Sign In
                </button>
            </form>
        </div>
        </section>
